{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": [
      "src/resources/users",
      "src/resources/tasks",
      "src/resources/boards",
      "src/resources/utils"
    ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "REST-service",
      "search": false
    }
  }
}
